{
    "name": "orc-design-ui",
    "private": false,
    "version": "1.0.9",
    "type": "module",
    "files": [
        "dist/**/*"
    ],
    "main": "./dist/orc-design-ui.umd.js",
    "module": "./dist/orc-design-ui.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/orc-design-ui.es.js",
            "require": "./dist/orc-design-ui.umd.js"
        }
    },
    "scripts": {
        "build:types": "tsc -p tsconfig.build.json",
        "dev": "vite",
        "build": "mv tsconfig.json tsconfig.dev.json && mv tsconfig.build.json tsconfig.json && vite build && mv tsconfig.json tsconfig.build.json && mv tsconfig.dev.json tsconfig.json",
        "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write .",
        "preview": "vite preview",
        "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "description": "Lib",
    "author": "grandpa-alex",
    "license": "MIT",
    "peerDependencies": {
        "@radix-ui/react-avatar": "^1.0.4",
        "@radix-ui/react-checkbox": "^1.0.4",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-popover": "^1.0.7",
        "@radix-ui/react-radio-group": "^1.1.3",
        "@radix-ui/react-scroll-area": "^1.1.0",
        "@radix-ui/react-select": "^2.0.0",
        "@radix-ui/react-slider": "^1.1.2",
        "@radix-ui/react-switch": "^1.0.3",
        "@radix-ui/react-tabs": "^1.1.0",
        "@radix-ui/react-toggle-group": "^1.0.4",
        "@radix-ui/react-tooltip": "^1.0.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^6.1.1"
    }
}
